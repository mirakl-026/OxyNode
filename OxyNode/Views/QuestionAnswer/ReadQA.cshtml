@model OxyNode.ViewModels.OneQAViewModel

@{
    ViewData["Title"] = "OxyNode | QA ";
}

<div>
    <table>

        <tr style="background-color:beige">
            <td>
                <div>@Model.qa.FullName,</div>
                <div>@Model.qa.Address</div>
            </td>
            <td>
                <div>Вопрос:</div>
                <div>@Model.qa.questionText</div>
            </td>
        </tr>

        <tr style="background-color:lightgreen">
            <td>
            </td>
            <td>
                <div>Ответ: </div>
                <div>@Model.qa.answerText</div>
            </td>
        </tr>

    </table>

</div>